<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
			
			<beans>
			
			
			<bean class="beans.Car" id="c">			
			<lookup-method name="getCarEngine" bean="e"/>
			</bean>
			
			
			<bean class="beans.Engine" id="e" >
			<property name="name" value="AudiEngine"/>
			</bean>
			
			
			<bean class="beans.Bus" id="b">			
			<lookup-method name="getBusEngine" bean="e1"/>
			</bean>
			
			
			<bean class="beans.Engine" id="e1" >
			<property name="name" value="VOLVO Engine"/>
			</bean>
			
			<bean class="beans.Truck" id="t">			
			<lookup-method name="getTruckEngine" bean="e2"/>
			</bean>
			
			
			<bean class="beans.Engine" id="e2" >
			<property name="name" value="TATA Truck Engine"/>
			</bean>
			
			
			
		
			
		
			
			</beans>